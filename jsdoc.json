{
    "source": {
      "include": [
        "src"
      ],
      "includePattern": ".js$",
      "excludePattern": "(node_modules|doc)"
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./doc",
      "template": "node_modules/clean-jsdoc-theme",
      "readme": "./README.md"
    }
  }